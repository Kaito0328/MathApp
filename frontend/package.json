{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
  "wasm:sync": "cp -f ../backend/crates/wasm/pkg/wasm.js src/wasm-pkg/wasm.js && cp -f ../backend/crates/wasm/pkg/wasm_bg.js src/wasm-pkg/wasm_bg.js && cp -f ../backend/crates/wasm/pkg/wasm_bg.wasm src/wasm-pkg/wasm_bg.wasm && cp -f ../backend/crates/wasm/pkg/wasm.d.ts src/wasm-pkg/wasm.d.ts",
    "wasm:build": "wasm-pack build ../backend/crates/wasm --target web --out-dir ../../frontend/src/wasm-pkg --out-name wasm",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "katex": "^0.16.22",
    "next": "^15.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.5.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jsdom": "^24.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vitest": "^1.6.0"
  }
}
